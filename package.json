{
  "name": "@luxsafe/workspace",
  "version": "1.0.0",
  "author": "Lux Partners Limited <hi@lux.partners> (https://lux.partners)",
  "license": "MIT",
  "keywords": [
    "ethereum",
    "react",
    "uniswap",
    "workspaces",
    "yarn"
  ],
  "private": true,
  "scripts": {
    "build": "yarn build:app",
    "build:app": "yarn workspace @luxmarket/app build",
    "build:contracts": "yarn workspace @luxmarket/contracts build",
    "build:sdk": "yarn workspace @luxmarket/sdk build",
    "build:ui": "yarn workspace @luxmarket/ui build",
    "dev": "yarn workspace @luxmarket/app dev"
  },
  "engines": {
    "node": ">=12",
    "yarn": ">=2"
  },
  "workspaces": {
    "packages": [
      "web"
    ],
    "nohoist": [
      "**/@openzeppelin/contracts-upgradeable/**",
      "**/@openzeppelin/contracts"
    ]
  },
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "@parcel/packager-ts": "2.8.3",
    "@parcel/transformer-typescript-types": "2.8.3",
    "typescript": ">=3.0.0"
  }
}
